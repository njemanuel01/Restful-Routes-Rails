<ul>
  <% @user.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
</ul>

<%= form_tag('/users', method: "post") do %>
  <input type="text" placeholder="New Email" name="users[email]" value="<%=@user.email%>"></input>
  <input type="password" placeholder="New Password" name="users[password]"></input>
  <input type="submit"></input>
<% end %>




